  "homepage": "https://github.com/Soitora/TMDB-PAS-Assistant#readme",
  "dependencies": {
    "@cospired/i18n-iso-languages": "^4.1.0",
    "clipboardy": "^3.0.0",
    "colors": "^1.4.0",
    "dotenv": "^16.0.3",
    "i18n-iso-countries": "^7.5.0",
    "js-yaml": "^4.1.0",
    "node-themoviedb": "^0.2.8",
    "pjson": "^1.0.9",
